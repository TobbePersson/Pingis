@model AddRoleVm

<h1>All users in @Model.Role</h1>

<table class="table">
    <tr>
        <th>Email</th>
    
        <th>User Name</th>
        
        <th>User Id</th>
    
    </tr>
    <tbody>
        @foreach (var item in Model.indentityusers)
        {
            <tr>
                <td>@item.Email</td>
            
                <td>@item.UserName</td>
            
                <td>@item.Id</td>
            </tr>
        }
    </tbody>
</table>

